{
    "name": "staudenmeir/laravel-migration-views",
    "description": "Laravel database migrations with SQL views",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Staudenmeir",
            "email": "mail@jonas-staudenmeir.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^13.0"
    },
    "require-dev": {
        "laravel/framework": "^13.0",
        "orchestra/testbench-core": "^11.0",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^12.5"
    },
    "autoload": {
        "psr-4": {
            "Staudenmeir\\LaravelMigrationViews\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Staudenmeir\\LaravelMigrationViews\\DatabaseServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
